<?xml version="1.0" encoding="UTF-8"?>
<DMX_CONTROL version="2.00" log="yes" port="2012" deviceProfile="default">
	<!-- ********************************************************************************************* -->
	<event type="NORMAL_CONDITION_1" continuous="yes">
		<!-- steady soft white light -->
		<timeblock mseconds="1000">
			<setvalue index="0" value="100" change="0"></setvalue>			<!-- red value of first light box -->
			<setvalue index="1" value="100" change="0"></setvalue>			<!-- green value of first light box -->
			<setvalue index="2" value="100" change="0"></setvalue>			<!-- blue value of first light box -->
